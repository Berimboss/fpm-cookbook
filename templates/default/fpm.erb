#!<%=@context[:interpreter]%>
<%optstr=''%>
<%combooptstr=''%>
<%@context[:bin_options_combos].each do |opt|%>
<%combooptstr << " #{opt[:symbol]} #{opt[:value]}"%>
<%end%>
<%@context[:bin_options].each do |opt|%>
<%optstr << " #{opt[:value]}"%>
<%end%>
system('<%=@context[:bin]%> <%=optstr%> <%=combooptstr%> <%=@context[:sources]%>')
system('mv <%=@context[:pkg_name]%> <%=@context[:simple_pkg_name]%>')
